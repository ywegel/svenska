    <!-- ## Verbs ## -->
    <string name="verbs_tab">Verbs</string>
    <string name="verbs_title">Swedish Verbs</string>
    <string name="group_1">Group 1</string>
    <string name="group_1_rule">Ends with *-a* in infinitive, present tense *-ar*, past tense *-ade*</string>
    <string name="group_1_example">tala → talar (present), talade (past)</string>
    <string name="group_2a">Group 2a</string>
    <string name="group_2a_rule">Imperative ends with voiced consonant, past tense *-de*</string>
    <string name="group_2a_example">bygga → bygger (present), byggde (past)</string>
    <string name="group_2b">Group 2b</string>
    <string name="group_2b_rule">Imperative ends with voiceless consonant, past tense *-te*</string>
    <string name="group_2b_example">köpa → köper (present), köpte (past)</string>
    <string name="group_3">Group 3</string>
    <string name="group_3_rule">Imperative and infinitive end with long stressed vowel, past tense *-dde*</string>
    <string name="group_3_example">bo → bor (present), bodde (past)</string>
    <string name="group_4">Group 4 (Irregular)</string>
    <string name="group_4_rule">Often with stem change, no fixed rule</string>
    <string name="group_4_example">skriva → skriver (present), skrev (past)</string>

    <!-- ## Nouns ## -->
    <string name="nouns_tab">Nouns</string>
    <string name="nouns_title">Swedish Nouns</string>
    <string name="group_or">Group 1 (-or)</string>
    <string name="group_or_rule">En-words that end with *-a*</string>
    <string name="group_or_example">en flicka → flickor</string>
    <string name="group_ar">Group 2 (-ar)</string>
    <string name="group_ar_rule">En-words with unstressed *-e*, *-el*, *-er* or certain consonants</string>
    <string name="group_ar_example">en cykel → cyklar</string>
    <string name="group_er">Group 3 (-er)</string>
    <string name="group_er_rule">En-words with stress on the last syllable or certain endings</string>
    <string name="group_er_example">en station → stationer</string>
    <string name="group_r">Group 4 (-r)</string>
    <string name="group_r_rule">One-syllable en-words that end with a stressed vowel</string>
    <string name="group_r_example">en sko → skor</string>
    <string name="group_n">Group 5 (-n)</string>
    <string name="group_n_rule">Ett-words that end with a vowel</string>
    <string name="group_n_example">ett kvitto → kvitton</string>
    <string name="group_unchanged">Group 6 (-)</string>
    <string name="group_unchanged_rule">Ett-words that end with a consonant, or certain en-words</string>
    <string name="group_unchanged_example">ett hus → hus, en lärare → lärare</string>

